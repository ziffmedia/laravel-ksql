{
    "name": "ziffmedia/laravel-ksql",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ziffmedia/ksql": "dev-timeout-handling"
    },

    "require-dev": {
        "orchestra/testbench": "^7.19",
        "laravel/pint": "^1.4",
        "pestphp/pest": "^1.22"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZiffMedia\\LaravelKsql\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Josh Butts",
            "email": "josh.butts@ziffmedia.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZiffMedia\\LaravelKsql\\KsqlServiceProvider"
            ]
        }
    }
}
